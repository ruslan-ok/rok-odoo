<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="dashboard_crypto" model="spreadsheet.dashboard">
        <field name="name">Crypto</field>
        <field name="spreadsheet_binary_data" type="base64" file="crypto/data/files/crypto_dashboard.json"/>
        <field name="main_data_model_ids" eval="[(4, ref('crypto.model_rok_crypto'))]"/>
        <field name="sample_dashboard_file_path">crypto/data/files/crypto_sample_dashboard.json</field>
        <field name="dashboard_group_id" ref="spreadsheet_dashboard.spreadsheet_dashboard_group_finance"/>
        <field name="group_ids" eval="[Command.link(ref('base.group_user'))]"/>
        <field name="sequence">90</field>
        <field name="is_published">True</field>
    </record>

</odoo>

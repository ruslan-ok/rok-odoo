<?xml version="1.0" encoding="utf-8"?>
<templates id="template" xml:space="preserve">
    <t t-name="weather.WeatherGraphRenderer" t-inherit="web.GraphRenderer" t-inherit-mode="primary">
        <xpath expr="//div[@t-ref='container']" position="replace">
            <div t-if="model.hasData()" class="o_graph_canvas_container flex-grow-1 position-relative px-3 pb-3" t-ref="container">
                <Weather />
            </div>
        </xpath>
    </t>
</templates>
